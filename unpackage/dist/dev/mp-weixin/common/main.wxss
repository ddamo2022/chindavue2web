	color: #333;
}
.weui-cells:before {
	position: absolute;
	content: '';
	top: 0;
	left: 0;
	right: 0;
	height: 1px;
	-webkit-transform: scaleY(0.5);
	        transform: scaleY(0.5);
	background-color: #eee;
}
.weui-cells:after {
	position: absolute;
	content: '';
	bottom: 0;
	left: 0;
	right: 0;
	height: 1px;
	-webkit-transform: scaleY(0.5);
	        transform: scaleY(0.5);
	background-color: #eee;
}
.weui-cells__title {
	margin-top: .77em;
	margin-bottom: .3em;
	padding-left: 15px;
	padding-right: 15px;
